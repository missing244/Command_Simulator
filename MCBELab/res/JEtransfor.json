    "IdentifierAndState":{
        "_button$":{
            "face": {"BEstate":"facing_direction", "floor":0, "ceiling":1, "wall":2},
            "facing": {"BEstate":"facing_direction", "north":2, "south":3, "west":4, "east":5}
        },
        "_slab$":{
            "type": {"BEstate":"top_slot_bit", "bottom":false, "top":true, "double":"Function(replaceID)"}
        },
        "_wall_hanging_sign$":{
            "facing": {"BEstate":"facing_direction", "north":2, "south":3, "west":4, "east":5}
        },
        "(activator_rail|detector_rail|golden_rail)$":{
            "shape":{"BEstate":"rail_direction", "north_south":0, "east_west":1, "ascending_east":2,
                "ascending_west":3, "ascending_north":4, "ascending_south":5}
        },
        "bamboo$":{
            "age": {"BEstate":"bamboo_stalk_thickness", "0":"thin", "1":"thick"},
            "leaves": {"BEstate":"bamboo_leaf_size", "none":"no_leaves", "small":"small_leaves", "large":"large_leaves"}
        },
        "comparator$":{
            "mode": {"BEstate":"output_subtract_bit", "compare":false, "subtract":true}
        },
        "rail$":{
            "shape":{"BEstate":"rail_direction", "north_south":0, "east_west":1, "ascending_east":2, "ascending_west":3, 
            "ascending_north":4, "ascending_south":5, "south_east":6, "south_west":7, "north_west":8, "north_east":9}
        },
        "_wall$":{
            "east": {"BEstate":"wall_connection_type_east", "none":"none", "low":"short", "tall":"tall"},
            "north": {"BEstate":"wall_connection_type_north", "none":"none", "low":"short", "tall":"tall"},
            "south": {"BEstate":"wall_connection_type_south", "none":"none", "low":"short", "tall":"tall"},
            "west": {"BEstate":"wall_connection_type_west", "none":"none", "low":"short", "tall":"tall"},
            "up": {"BEstate":"wall_post_bit", "true":true, "false":false}
        },
        "_door$":{
            "facing": {"BEstate":"direction", "south":1, "west":2, "north":3, "east":0}
        },
        "_trapdoor$":{
            "facing": {"BEstate":"direction", "south":2, "west":1, "north":3, "east":0}
        },
        "tall_seagrass$":{
            "half": {"BEstate":"sea_grass_type", "lower":"double_bot", "upper":"double_top"}
        },
        "lever$":{
            "facing": {"BEstate":"lever_direction", "north":"north", "south":"south", "west":"west", "east":"east"},
            "face": {"BEstate":"lever_direction", "floor":"up_east_west", "ceiling":"down_east_west"}
        }
